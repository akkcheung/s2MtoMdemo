        <action name="index">
            <result>/index.jsp</result>
        </action>
        
        <!--
         <action name="hello" class="org.apache.struts.helloworld.action.HelloWorldAction" method="execute">
          <result name="success">/HelloWorld.jsp</result>
        </action>
